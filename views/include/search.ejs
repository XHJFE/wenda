
<!-- 搜索 -->
<div class="wd_search_wrap">
    <div>
        <div class="input_wrap">
            <input type="text" placeholder="房产问题与疑问，一搜即答" id="searchInput"/>
            <a href="javascript:void(0);" onclick="submitSearch()"><img src="../images/wd_ss_btn.png" /></a>
        </div>

        <a href="/ask_questions" class="wd_tw_btn"><img src="../images/wd_tw_btn.png" /></a>
    </div>
</div>

<script>
    function submitSearch() {
        var search = $('#searchInput').val()
        if (!search) {
            return
        }
        window.location.href = '/search_result?problemTitle='+search+''
    }
</script>